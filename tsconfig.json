{
    "compilerOptions": {
        "target": "ES2017",
        "module": "Node16",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "outDir": "out",
        "resolveJsonModule": true
    },
    "include": ["src/**/*"],
    "exclude": ["**/*.spec.ts"]
}
